{
  "name": "@wordle/core",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node ./dist/cli/index.js",
    "dev": "tsx src/cli/index.ts",
    "precompute": "tsx src/cli/index.ts --precompute",
    "solve": "tsx src/cli/index.ts --mode=hardcore",
    "solve:full": "tsx src/cli/index.ts --mode=full",
    "generate:initial": "tsx scripts/generateInitialSuggestions.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./lib": {
      "import": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./browser": {
      "import": "./dist/lib/browser.js",
      "types": "./dist/lib/browser.d.ts"
    }
  },
  "dependencies": {
    "@types/node": "^22.7.5",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3"
  }
}